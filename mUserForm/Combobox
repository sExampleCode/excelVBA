'1보다 큰경우
    If rngDivision.Rows.Count > 1 Then
        '구분 초기화
        With Me.cbDivision
            .RowSource = rngDivision.Offset(1).Resize(rngDivision.Rows.Count - 1).Address(External:=True)
            .ListIndex = 0
            .Style = fmStyleDropDownList
            .ListStyle = fmListStyleOption
        End With
        
        Call AutoFit_Border(rngDivision)
    End If
